    "@types/jest": "^25.2.1",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0",
    "concurrently": "^5.2.0",
    "cypress": "^4.5.0",
    "jest": "^25.5.4",
    "jest-preset-angular": "^8.2.0",
    "json-server": "^0.16.1",
    "moment": "^2.24.0",
    "prettier": "^2.0.5",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "ts-jest": "^25.5.1",
    "typescript": "^3.9.6"
  }
